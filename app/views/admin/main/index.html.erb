


<style>

body{
background-color: black;
color: greenyellow;
}

td,th {
background: -webkit-linear-gradient(#eee, #333);
-webkit-background-clip: text;
-webkit-text-fill-color: transparent;
}
button a{
text-decoration: none !important;
color: white !important;
}
.table{
color: greenyellow;
}

</style>


<div class="container">
<h1>Your Sellers</h1>
<table class="table">
<thead>
<tr>
<th scope="col">ID</th>
<th scope="col">Email</th>
<th scope="col">First Name</th>
<th scope="col">Last Name</th>
<th scope="col">Age</th>
<th scope="col">Gender</th>
<th scope="col">Phone</th>
<th scope="col">Address</th>
<th scope="col">Edit</th>
<th scope="col">Delete</th>
</tr>
</thead>
<tbody>
<% @sellers.each do |ac|%>


<tr>
<th scope="row"><%= ac.id %></th>
<td><%= ac.firstname%></td>
<td><%= ac.lastname%></td>
<td><%= ac.email%></td>
<td><%= ac.age%></td>
<td><%= ac.gender%></td>
<td><%= ac.phone%></td>
<td><%= ac.address%></td>
</tr>
<% end %>
</tbody>

</table>


<h1>Your Customers</h1>
<table class="table">
<thead>
<tr>
<th scope="col">ID</th>
<th scope="col">Email</th>
<th scope="col">First Name</th>
<th scope="col">Last Name</th>
<th scope="col">Age</th>
<th scope="col">Gender</th>
<th scope="col">Phone</th>
<th scope="col">Address</th>
<th scope="col">Edit</th>
<th scope="col">Delete</th>
</tr>
</thead>
<tbody>


<% @customers.each do |ca|%>
<tr>
<th scope="row"><%= ca.id %></th>
<td><%= ca.firstname%></td>
<td><%= ca.lastname%></td>
<td><%= ca.email%></td>
<td><%= ca.age%></td>
<td><%= ca.gender%></td>
<td><%= ca.phone%></td>
<td><%= ca.address%></td>
</tr>
<% end %>
</tbody>

</table>
</div>
